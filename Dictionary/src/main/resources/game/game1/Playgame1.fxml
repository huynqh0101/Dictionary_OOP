<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="522.0" prefWidth="789.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.game.game1.Game1Controller">
    <children>
        <Button fx:id="Backfromplaygametogame1" layoutY="1.0" mnemonicParsing="false" onMouseClicked="#Backtogame1" text="Back" />
      <VBox layoutX="139.0" layoutY="103.0" prefHeight="245.0" prefWidth="511.0" spacing="13.0">
         <children>
            <Label fx:id="questiongame" prefHeight="57.0" prefWidth="491.0" text="Question" wrapText="true">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <CheckBox fx:id="a" mnemonicParsing="false" onMouseClicked="#ChooseanswerA" prefHeight="25.0" prefWidth="500.0" text="CheckBox" wrapText="true">
               <font>
                  <Font size="13.0" />
               </font></CheckBox>
            <CheckBox fx:id="b" mnemonicParsing="false" onMouseClicked="#ChooseanswerB" prefHeight="25.0" prefWidth="500.0" text="CheckBox" wrapText="true">
               <font>
                  <Font size="13.0" />
               </font></CheckBox>
            <CheckBox fx:id="c" mnemonicParsing="false" onMouseClicked="#ChooseanswerC" prefHeight="25.0" prefWidth="500.0" text="CheckBox" wrapText="true">
               <font>
                  <Font size="13.0" />
               </font></CheckBox>
            <CheckBox fx:id="d" mnemonicParsing="false" onMouseClicked="#ChooseanswerD" prefHeight="25.0" prefWidth="500.0" text="CheckBox" wrapText="true">
               <font>
                  <Font size="13.0" />
               </font></CheckBox>
         </children>
      </VBox>
      <HBox layoutX="252.0" layoutY="390.0" prefHeight="25.0" prefWidth="285.0" spacing="15.0">
         <children>
            <Button fx:id="back_question" mnemonicParsing="false" onMouseClicked="#Back_question" text="&lt;-" />
            <Label fx:id="score" prefHeight="21.0" prefWidth="119.0" text="Score:">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Button fx:id="submitanswer" mnemonicParsing="false" onMouseClicked="#Submitanswer" text="Submit" />
            <Button fx:id="next_question" mnemonicParsing="false" onMouseClicked="#Next_question" text="-&gt;" />
         </children>
      </HBox>
      <Button fx:id="restart" layoutX="60.0" layoutY="1.0" mnemonicParsing="false" onMouseClicked="#Restart" text="Restart" />
    </children>
</AnchorPane>
